    "agora-rtc-sdk-ng": "^4.x.x",
    "concurrently": "^9.1.0",
    "lucide-react": "^0.454.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.41.0",
    "mongoose": "^8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5"
  }
}
