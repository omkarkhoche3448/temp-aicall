{
  "name": "video-call-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev-client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev-chatbot":"cd ../chatbot && php artisan serve --host=localhost --port=8000",
    "dev-server": "cd ../videocallBackend && npm run dev",
